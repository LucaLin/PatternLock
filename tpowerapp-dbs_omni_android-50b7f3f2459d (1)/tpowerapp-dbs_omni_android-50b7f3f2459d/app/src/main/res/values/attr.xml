<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputTextView">
        <attr name="textTitle" format="string"/>
        <attr name="AmountSign" format="string"/>
        <attr name="hasSelectButton" format="boolean"/>
        <!--<attr name="android:inputType"/>-->
        <!--<attr name="android:digits"/>-->
        <!--<attr name="android:maxLength"/>-->
        <!--<attr name="android:singleLine"/>-->
        <!--<attr name="android:imeOptions"/>-->
        <attr name="inputTextType" format="enum">
            <enum name="None" value="0"/>
            <enum name="UserID" value="1"/>
            <enum name="Amount" value="2"/>
            <enum name="CreditCard" value="3"/>
            <enum name="EffectiveDate" value="4"/>
            <enum name="BirthDate" value="5"/>
            <enum name="Password" value="6"/>
            <enum name="City" value="7"/>
            <enum name="Region" value="8"/>
            <enum name="NickName" value="9"/>
            <enum name="UserAccount" value="10"/>
            <enum name="Email" value="11"/>
            <enum name="PhoneNumber" value="12"/>
            <enum name="Display" value="13"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="SymbolItem">
        <attr name="textContent" format="string"/>
    </declare-styleable>

</resources>